pipeline {
      agent any
stages{
stage('test') { 
parallel { 
stage ('test1')  { steps { echo "this is build" } }
stage ('test2')  { steps { echo "this is test2" } }
}
}
stage('build') { 
parallel {
stage ('build1') { steps { echo "this is build" } }
stage ('build2') { steps { echo "this is build" } }
}
 }
}
}
